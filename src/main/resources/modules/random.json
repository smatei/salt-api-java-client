{
  "random.rand_int": {
    "args": [
      "start",
      "end"
    ],
    "defaults": [
      1,
      10
    ]
  },
  "random.get_str": {
    "args": [
      "length"
    ],
    "defaults": [
      20
    ]
  },
  "random.shadow_hash": {
    "args": [
      "crypt_salt",
      "password",
      "algorithm"
    ],
    "defaults": [
      null,
      null,
      "sha512"
    ]
  },
  "random.hash": {
    "args": [
      "value",
      "algorithm"
    ],
    "defaults": [
      "sha512"
    ]
  },
  "random.seed": {
    "args": [
      "range",
      "hash"
    ],
    "defaults": [
      10,
      null
    ]
  },
  "random.str_encode": {
    "args": [
      "value",
      "encoder"
    ],
    "defaults": [
      "base64"
    ]
  }
}