{
  "jboss7_cli.run_command": {
    "args": [
      "jboss_config",
      "command",
      "fail_on_error"
    ],
    "defaults": [
      true
    ]
  },
  "jboss7_cli.run_operation": {
    "args": [
      "jboss_config",
      "operation",
      "fail_on_error",
      "retries"
    ],
    "defaults": [
      true,
      1
    ]
  }
}