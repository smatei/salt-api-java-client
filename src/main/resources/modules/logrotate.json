{
  "logrotate.get": {
    "args": [
      "key",
      "value",
      "conf_file"
    ],
    "defaults": [
      null,
      "/etc/logrotate.conf"
    ]
  },
  "logrotate.show_conf": {
    "args": [
      "conf_file"
    ],
    "defaults": [
      "/etc/logrotate.conf"
    ]
  },
  "logrotate.set": {
    "args": [
      "key",
      "value",
      "setting",
      "conf_file"
    ],
    "defaults": [
      null,
      "/etc/logrotate.conf"
    ]
  }
}